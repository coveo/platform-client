{
    "name": "@coveo/platform-client",
    "version": "0.0.0-development",
    "description": "",
    "main": "dist/Entry.js",
    "types": "dist/definitions/Entry.d.ts",
    "keywords": [
        "coveo",
        "api",
        "client",
        "platform",
        "front-end",
        "web"
    ],
    "scripts": {
        "start": "tsc -p tsconfig.build.json --watch",
        "build": "eslint \"src/**.*\" --ignore-pattern '**/*.spec.*' --color && tsc -p tsconfig.build.json",
        "test": "jest --clearCache && jest",
        "test:changed": "jest --watch",
        "test:watch": "jest --watchAll",
        "release": "semantic-release",
        "commit-cli": "git-cz",
        "lintfix": "prettier --write \"**/*.{ts,js,json,md,yml}\" && eslint -c ./.eslintrc.js \"src/**.*\" --fix",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "17.3.0",
        "@commitlint/config-conventional": "17.3.0",
        "@types/jest": "29.2.3",
        "@typescript-eslint/eslint-plugin": "5.44.0",
        "@typescript-eslint/parser": "5.44.0",
        "abortcontroller-polyfill": "1.7.5",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-typescript": "3.0.0",
        "eslint-plugin-jest": "27.1.5",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.2",
        "jest": "29.3.1",
        "jest-fetch-mock": "3.0.3",
        "jest-runner-eslint": "1.1.0",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "semantic-release": "19.0.5",
        "ts-jest": "29.0.3",
        "tsjs": "4.2.1",
        "tslib": "2.4.1",
        "typescript": "4.9.3"
    },
    "prettier": "tsjs/prettier-config",
    "lint-staged": {
        "**/*.{ts,js}": [
            "prettier --write",
            "eslint -c ./.eslintrc.js --fix"
        ],
        "**/*.{md,json,yml}": [
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/coveo/platform-client.git"
    },
    "author": "Coveo",
    "license": "Apache-2.0",
    "homepage": "https://github.com/coveo/platform-client#readme",
    "files": [
        "/dist"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "exponential-backoff": "^3.1.0",
        "form-data": "^4.0.0",
        "query-string": "^7.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
